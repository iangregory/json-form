{
    "name": "mcfedr/json-form",
    "description": "A couple of helper files for handling json with symfony forms",
    "keywords": ["json", "forms", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "src/Mcfedr/JsonFormBundle" }
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "tests/Mcfedr/JsonFormBundle" },
        "classmap": ["tests/TestKernel.php"]
    },
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^3.0|^4.0|^5.0",
        "symfony/form": "^3.0|^4.0|^5.0",
        "symfony/validator": "^3.0|^4.0|^5.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/monolog-bundle": "^3.0|^4.0",
        "friendsofphp/php-cs-fixer": "^2.1",
        "symfony/browser-kit": "^3.0|^4.0",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-symfony": "^0.11.2",
        "phpstan/phpstan-doctrine": "^0.11.1",
        "symfony/phpunit-bridge": "*",
        "symfony/yaml": "^3.0|^4.0|^5.0"
    },
    "extra": {
        "symfony": {
            "require": "^4.2|^5.0"
        }
    }
}
